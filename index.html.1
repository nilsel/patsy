    <script type="text/javascript">
      (function($){
        $.fn.clickSlide=function(pagesDiv){
          var width=$(pagesDiv).css({
              position:"relative"
            }).width(),
            pages=$(pagesDiv).children().css({
              width:width,
              minWidth:width,
              display:"table-cell"
            })
          pages.hide().eq(0).show()
          //$(pagesDiv).width(width*2)
          
          return this.click(function(){
            var $visible=pages.filter(':visible')
            var $page=$($(this).attr('data-rel'))
            if (!$page.is(':visible')){
              $page.show()
              $(pagesDiv).stop(true,false)
                .css('left',-$visible.position().left)
                .animate({left:-$page.position().left},'slow',function(){
                  $visible.hide()
                  $(this).css('left',0)
                })
            }
            return false
          })
          
        }

        $(function(){
          $('.menu a').clickSlide('#slides');
          $('.menu button').clickSlide('#slides');
          SB.start();
          SB.getContracts(SB.populateFPIList);
          SB.getSimulation(SB.popuplateSimulationView);
          SB.browsePages('.page','#poliser');
       //   SB.testChart('simulation1_graph');
        });

        
      }(jQ));
