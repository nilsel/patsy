{
  "project": {
    "name": "ProjectFoo",
    "webroot": "/",
    "path": "C:\\Users\\JohnDoe\\Workspace\\ProjectFoo\\",
    "project_folder": "ProjectFoo",
    "author" : "John Doe",
    "project_site" : "http://projectfoo.com",
    "options" : {
      "watch_config": true
    }
  },
  "build": {
    "js": "js/src/",
    "min": {
      "dest": "js/min/",
      "options": {
        "sourcemap": true,
        "allinone": false,
        "verbose": true
      }
    },
    "dist": "js/dist/",
    "tmpl": {
      "src": "js/mustache/",
      "options" : {
        "prefix" : "$B.extend({ templates: ",
        "postfix" : "});",
        "verbose": false
      }
    },
    "lint": {
      "options": {}
    },
    "docs": {
      "dest": "js/docs/",
      "options": {}
    },
    "css" : {
      "src" : "css/src/",
      "dist" : "css/dist/",
      "options" : {
        "compile" : true,
        "compress" : true
      }
    },
    "test": {
      "suites": {
        "jasmine" : {
          "src": "js/test/jasmine/**/*.js"
        },"qunit" : {
          "src": "js/test/qunit/**/*.js",
          "options": {
            "timeout": 10000,
            "--cookies-file": "misc/cookies.txt"
          }
        },"nodeunit" : {
          "src": "js/test/nodeunit/**/*.js"
        }
      }
    },
    "options": {
      "verbose": true
    }
  },
  "proxy": {
    "resources": [],
    "options": {
      "verbose": true
    }
  },
  "options": {
    "verbose": true,
    "logToFile": false,
    "scripture": true
  }
}
